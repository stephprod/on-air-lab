<% include header %>


    <section class="section-dark no-padding">
		<!-- Slider main container -->
		<div id="swiper-gallery" class="swiper-container">
			<!-- Additional required wrapper -->
			<div class="swiper-wrapper">
				<!-- Slides -->
				<% for (k in locals.documentsObj){ %>
				<div class="swiper-slide">
					<div class="slide-bg swiper-lazy" data-background="<%= locals.documentsObj[k].path %>" data-sub-html="<strong>this is a caption 1</strong><br/>Second line of the caption"></div>
					<!-- Preloader image -->
					<div class="swiper-lazy-preloader swiper-lazy-preloader-white"></div>
					<div class="container">
                        <div class="row">
                            <div class="col-sm-12 col-md-9 col-lg-8 slide-desc-col animated fadeInUp gallery-slide-desc-<%= (k + 1) %>">
								<div class="gallery-slide-cont">
									<div class="gallery-slide-cont-inner">
										<div class="gallery-slide-title pull-right">
											<h5 class="subtitle-margin">apartments for sale</h5>
											<h3><%= locals.profilObj[0].nom %>, <%= locals.profilObj[0].adresse %>, <%= locals.profilObj[0].cp %><span class="special-color">.</span></h3>
										</div>
										<div class="gallery-slide-estate pull-right hidden-xs">
											<i class="fa fa-home"></i>
										</div>
										<div class="clearfix"></div>
									</div>
									<div class="clearfix"></div>
									<div class="gallery-slide-desc-price pull-right">
										€<%=locals.tarificationObj[0].prix_h %>/h
									</div>
									<div class="clearfix"></div>
								</div>
							</div>
						</div>
					</div>
				</div>
				<% } %>
			</div>

			<div class="slide-buttons slide-buttons-center">
				<a href="#" class="navigation-box navigation-box-next slide-next"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe802;</i></div></a>
				<div id="slide-more-cont"></div>
				<a href="#" class="navigation-box navigation-box-prev slide-prev"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe800;</i></div></a>
			</div>

		</div>

    </section>
	<section class="thumbs-slider section-both-shadow">
		<div class="container">
			<div class="row">
				<div class="col-xs-1">
					<a href="#" class="thumb-box thumb-prev pull-left"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe800;</i></div></a>
				</div>
				<div class="col-xs-10">
					<!-- Slider main container -->
					<div id="swiper-thumbs" class="swiper-container">
						<!-- Additional required wrapper -->
						<div class="swiper-wrapper">
							<!-- Slides -->
							<% for (k in locals.documentsObj){ %>
								<div class="swiper-slide">
									<img class="slide-thumb" src="<%= locals.documentsObj[k].path %>" alt="">
								</div>
							<% } %>
						</div>
					</div>
				</div>
				<div class="col-xs-1">
					<a href="#" class="thumb-box thumb-next pull-right"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe802;</i></div></a>
				</div>
			</div>
		</div>
	</section>
	<section class="section-light no-bottom-padding">
		<div data-name="global_msg" class="msg-contact"></div>
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
					<div class="row">
						<div class="col-xs-12 col-sm-5 col-md-4 col-lg-3" style="margin-bottom: 36px;">
							<% if (locals.servicesObj.length != 0) { %>
							<div class="details-parameters-price">à partir de <%=locals.tarificationObj[0].prix_min %>€</div>
							<div class="details-parameters">
								<% for (var i = locals.servicesObj.length - 1; i >= 0; i--) { %>
								<div class="details-parameters-cont">
									<div class="details-parameters-name"><%= locals.servicesObj[i].nom_service %></div>
									<div class="details-parameters-val"><%=locals.tarificationObj[0].nbr_h_min %>h min.</div>
									<div class="clearfix"></div>
								</div>
								<% } %>
							</div>
							<% } else{ %>
									<div class="details-parameters-price">Aucun service.</div>
							<% } %>
								<div class="center-button-cont margin-top-15">
									<% if (locals.session.userId !== undefined && locals.session.userId != null){ %>
										<form id="contact" method="POST" action="/profile/<%=locals.session.id_u%>">
											<button type="submit" style="border: none;" class="button-primary button-shadow button-full">
												<span>Contacter</span>
												<div class="button-triangle"></div>
												<div class="button-triangle2"></div>
												<div class="button-icon"><i class="far fa-comments"></i></div>
											</button>
										</form>
									<% }else{ %>
										<a href="#login-modal" class="button-primary button-shadow button-full" data-toggle="modal">
											<span>Contacter</span>
											<div class="button-triangle"></div>
											<div class="button-triangle2"></div>
											<div class="button-icon"><i class="far fa-comments"></i></div>
										</a>
									<% } %>
								</div>
								<% if (locals.profilObj[0].type == 3){ %>
									<div class="center-button-cont margin-top-15">
										<a <% if (locals.session.userId !== undefined && locals.session.userId != null){ %>href="#rdv-modal"<%} else {%> href="#login-modal" <%}%> data-toggle="modal" class="button-alternative button-shadow button-full">
											<span>Prendre Rendez-vous</span>
											<div class="button-triangle"></div>
											<div class="button-triangle2"></div>
											<div class="button-icon"><i class="far fa-calendar-alt"></i></div>
										</a>
									</div>
								<% }else{ %>
									<div class="center-button-cont margin-top-15">
										<a <%if (locals.session.userId !== undefined && locals.session.userId != null){ %>href="#resa-modal"<%} else {%> href="#login-modal" <%}%> data-toggle="modal" class="button-alternative button-shadow button-full">
											<span>Reserver studio</span>
											<div class="button-triangle"></div>
											<div class="button-triangle2"></div>
											<div class="button-icon"><i class="far fa-calendar-check"></i></div>
										</a>
									</div>
								<% } %>
						</div>
						<div class="col-xs-12 col-sm-7 col-md-8 col-lg-9">
							<div class="details-image pull-left hidden-xs">
								<i class="fa fa-home"></i>
							</div>
							<div class="details-title pull-left">
								<h5 class="subtitle-margin">apartment for sale</h5>
								<h3><%= locals.profilObj[0].nom %>, <%= locals.profilObj[0].adresse %>, <%= locals.profilObj[0].cp %><span class="special-color">.</span></h3>
							</div>
							<div class="clearfix"></div>
							<div class="title-separator-primary"></div>
							<p class="details-desc"><%= locals.profilObj[0].descr %></p>
						</div>
					</div>
					<div class="row margin-top-45">
							<% for (i in locals.servicesObj){
								if (i == 0){ %>
									<div class="col-xs-6 col-sm-4">
										<ul class="details-ticks">
								<% } if (i > 0 && i % 4 == 0){ %>
										</ul>
									</div>
									<div class="col-xs-6 col-sm-4">
										<ul class="details-ticks">
								<% } %>
								<li><i class="jfont">&#xe815;</i><%= locals.servicesObj[i].nom_service %></li>
								<% if (i == locals.servicesObj.length - 1){ %>
									</div>
										</ul>
								<% } %>
						<% } %>
					</div>

					<div class="row margin-top-90">
						<div class="col-xs-12 col-sm-9">
							<h5 class="subtitle-margin">offres du moment</h5>
									<h1>liste<span class="special-color">.</span></h1>
						</div>
						<div class="col-xs-12 col-sm-3">
							<a href="#" class="navigation-box navigation-box-next" id="grid-offers-owl-next"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe802;</i></div></a>
							<a href="#" class="navigation-box navigation-box-prev" id="grid-offers-owl-prev"><div class="navigation-triangle"></div><div class="navigation-box-icon"><i class="jfont">&#xe800;</i></div></a>
						</div>
						<div class="col-xs-12">
							<div class="title-separator-primary"></div>
						</div>
					</div>

					<div class="grid-offers-container">
						<% if (locals.offObj.length != 0){ %>
						<div class="owl-carousel" id="grid-offers-owl">
							<% for (var i = locals.offObj.length - 1; i >= 0; i--) { %>
							<div class="grid-offer-col">
								<div class="grid-offer">
									<div class="grid-offer-front">

										<div class="grid-offer-photo">
											<img src="/asset/images/grid-offer1.jpg" alt="" />
											<div class="type-container">
												<%if (locals.offObj[i].type_service == "audio"){%>
													<div class="transaction-type">audio</div>
												<% }else{ %>
													<div class="estate-type">vidéo</div>
												<% } %>
												<div class="etiquette-name"><%= locals.offObj[i].titre %></div>
											</div>
										</div>
										<div class="grid-offer-text">
											<i class="fa fa-map-marker grid-offer-localization"></i>
											<div class="grid-offer-h4"><h4 class="grid-offer-title"><%= locals.profilObj[0].adresse %>, <%= locals.profilObj[0].cp %>, FRANCE</h4></div>
											<div class="clearfix"></div>
											<p><%= locals.offObj[i].spe_off %></p>
											<div class="clearfix"></div>
										</div>
										<div class="price-grid-cont">
											<div class="grid-price-label pull-left">Price:</div>
											<div class="grid-price pull-right">
												€ <%= locals.offObj[i].prix_off %>
											</div>
											<div class="clearfix"></div>
										</div>
										<div class="grid-offer-params">
											<div class="grid-area">
												<img src="/asset/images/area-icon.png" alt="" />54m<sup>2</sup>
											</div>
											<div class="grid-rooms">
												<img src="/asset/images/rooms-icon.png" alt="" />3
											</div>
											<div class="grid-baths">
												<img src="/asset/images/bathrooms-icon.png" alt="" />1
											</div>
										</div>

									</div>
									<div class="grid-offer-back">
										<div id="grid-map1" class="grid-offer-map"></div>
										<div class="button">
											<a id="<%= locals.offObj[i].id_offre %>" <%if (locals.session.userId !== undefined && locals.session.userId != null){ %> href="#rdv-offer-modal"<%} else {%> href="#login-modal" <%}%> data-action="in-chat" data-toggle="modal" class="button-primary">
												<span>Prise de rendez-vous</span>
												<div class="button-triangle"></div>
												<div class="button-triangle2"></div>
												<div class="button-icon"><i class="fa fa-search"></i></div>
											</a>
										</div>
									</div>
								</div>
							</div>
							<% } %>
						</div>
						<% }else{ %>
							Aucune offre.
						<% } %>
					</div>
					<div class="margin-top-45"></div>
				</div>
			</div>
		</div>
	</section>
	<footer class="small-cont">
	  <div class="container">
	    <div class="row">
	      <div class="col-xs-12 col-md-6 small-cont">
	      </div>
	      <div class="col-xs-12 col-md-6 footer-copyrights">
	        &copy; Copyright 2018 <a href="https://label-onair.com" target="blank">Label On Air</a>. All rights reserved.
	      </div>
	    </div>
	  </div>
	</footer>
	</div>

