	<script>
    	var user = {};
    	var session = {};
    	session.page = "<%- locals.page !== undefined ? locals.page : "null" %>";
    	session.id_u = "<%- locals.session.id_u !== undefined ? locals.session.id_u : "null" %>";
    	session.id_coresp = "<%- locals.session.id_coresp !== undefined ? locals.session.id_coresp : "null" %>";
		session.token = "<%- locals.session.token !== undefined ? locals.session.token : "null" %>";
		user.id = "<%- locals.session.userId != undefined ?
    		locals.session.userId : "null" %>";
    	user.nom = "<%- locals.session.userName ?
    		locals.session.userName : "null" %>";
    	user.type = "<%- locals.session.userType ?
    		locals.session.userType : "null" %>";
    	session.user = user;
    	var room, user_receiv = {};
    	room = "<%- locals.room !== null ? locals.room : "null" %>";
    	user_receiv.id_coresp = "<%- locals.session.user_receiv.id_coresp != undefined ?
    		locals.session.user_receiv.id_coresp : "null" %>";
    	user_receiv.nom = "<%- locals.session.user_receiv.nom ?
    		locals.session.user_receiv.nom : "null" %>";
    	user_receiv.prenom = "<%- locals.session.user_receiv.prenom ?
    		locals.session.user_receiv.prenom : "null" %>";
    	user_receiv.type = "<%- locals.session.user_receiv.type ?
			locals.session.user_receiv.type : "null" %>";
		user_receiv.pay_module = "<%- locals.session.user_receiv.pay_module ?
			locals.session.user_receiv.pay_module : "null" %>";
		user_receiv.mail = "<%- locals.session.user_receiv.mail ?
    		locals.session.user_receiv.mail : "null" %>";
    	sessionStorage.setItem("room", JSON.stringify(room));
    	sessionStorage.setItem("user_receiv", JSON.stringify(user_receiv));
    	sessionStorage.setItem("session", JSON.stringify(session));
    </script>
