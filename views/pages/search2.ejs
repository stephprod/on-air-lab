    </section>

	<section class="section-light section-top-shadow" id="res">
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
						<div class="row">
							<div class="col-xs-12 col-lg-6">
								<h5 class="subtitle-margin">Recherche de <span class="filter_flags">'Tous'</span> :</h5>
								<h1><span class="len"><%= locals.listEtab.length %></span> professionnel(s) trouvé(s)<span class="special-color">.</span></h1>
							</div>
							<div class="col-xs-12 col-lg-6">
								<div class="view-icons-container">
									<a class="view-box view-box-active"><img src="/asset/images/grid-icon.png" alt="" /></a>
									<a class="view-box" href="listing-list-right-sidebar.html"><img src="/asset/images/list-icon.png" alt="" /></a>
								</div>
								<div class="order-by-container">
									<select name="sort" class="bootstrap-select" title="Trier par:">
