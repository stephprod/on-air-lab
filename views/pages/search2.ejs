										<option data-sort-value="distance" data-sort-direction="desc">Distance décroissante</option>
										<option data-sort-value="name" data-sort-direction="asc">Nom croissant</option>
										<option data-sort-value="name" data-sort-direction="desc">Nom décroissant</option>
									</select>
								</div>
							</div>
							<div class="col-xs-12">
								<div class="title-separator-primary"></div>
							</div>
						</div>
						<div class="row">Affichage des éléments allant de <span class="first-page-ind"></span> à <span class="last-page-ind"></span> sur <span class="total-ind"></span></div>
						<div class="row grid-offer-row" id="etabs" style="min-height: 450px;">
							<% if (locals.listEtab !== undefined) {
				                for (k in locals.listEtab){
				            %>
				                  <% include pro_card2 %>

				            <%  }
				              } else { %>
				                Aucune enseigne pour le moment, actualisez la page !
				            <% } %>
						</div>
			</div>
		</div>
