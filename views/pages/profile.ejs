		<div class="col-md-3 img_user">
			<img src="/asset/images/blog-author-example.jpg" class="user_sz">
		</div>
		<div class="col-md-6">
			<h1 style="color: #185487;"><%= locals.profilObj[0].nom %></h1>
			<p class="text_head_sz"><i class="fas fa-home" style="padding: 10px;"></i><%= locals.profilObj[0].adresse %></p>
